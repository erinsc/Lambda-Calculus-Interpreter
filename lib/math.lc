# Booleans
\ab.a
= TRUE
\ab.b
= FALSE
\a.a FALSE TRUE
= NOT
\ab.aba
= AND
\ab.aab
= OR
\ab.AND (OR a b) (NOT (AND a b))
= XOR

# Math
\ab.b
= ZERO
\ab.a b
= ONE
\ab.a(ab)
= TWO
\ab.a(a(ab))
= THREE
\ab.a(a(a(ab)))
= FOUR
\abc.b(abc)
= INC
\ab.a INC b
= ADD
\ab.a (ADD b) ZERO
= MULT
\ab.ba
= EXP
\abc.a(\de.e(db))(\f.c)(\g.g)
= DEC
\ab.b DEC a
= SUB

# Flow control
\a.a (\b. FALSE) TRUE
= ISZERO
\ab. ISZERO (SUB a b)
= LEQ
\ab. ISZERO (SUB (INC a) b)
= LT
\ab. AND (LEQ a b) (LEQ b a)
= EQ
